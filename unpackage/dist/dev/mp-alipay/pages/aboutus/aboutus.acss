
.content{
	padding: 15px;
}
.Edition{
	color: #9DA6A0;
	    font-size: 14px;
	    height: 60px;
	    line-height: 60px;
}
.vlist{
	height: 50px;
	    /* background: pink; */
	    line-height: 50px;
	    font-size: 14px;
}
.vlist text:nth-child(1){
	color: #303331;
	    font-size: 14px;
	    font-weight: 700;
	    float: left;
}
.vlist text:nth-child(3){
	/* color: #9DA6A0; */
	    float: right;
}
.vlist image{
	width: 7px;
	    height: 12px;
	    float: right;
	    margin-top: 18px;
	    margin-left: 10px;
}
.new{
	width: 16px;
	    height: 16px;
	    background: red;
	    line-height: 15px;
	    display: block;
	    color: #fff;
	    font-size: 12px;
	    text-align: center;
	    border-radius: 50%;
	    padding: 1px;
	    margin-top: 15px;
}

