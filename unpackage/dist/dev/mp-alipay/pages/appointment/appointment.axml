<view class="content"><block a:for="{{AppointmentList}}" a:for-item="item" a:for-index="index" a:key="index"><view class="AppointmentListBox"><view class="AppointmentTime">{{item.Time}}</view><block a:for="{{item.List}}" a:for-item="list" a:for-index="i" a:key="i"><view class="AppointmentList"><view class="AppointmentListLeft"><text>{{list.time}}</text><text>打电话</text></view><view class="AppointmentListMid"><text class="AppointmentListName">{{list.name}}</text><view class="AppointmentListCon"><text>{{list.tag1}}</text><text>{{list.tag2}}</text><text>{{list.tag3}}</text></view></view><view data-event-opts="{{[['tap',[['AppointmentDetail',['$event']]]]]}}" class="AppointmentListRight iconfont" onTap="__e"></view></view></block></view></block></view>