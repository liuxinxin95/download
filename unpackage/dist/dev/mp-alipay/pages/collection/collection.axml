<view class="content"><block a:for="{{Reportlist}}" a:for-item="item" a:for-index="index" a:key="index"><uni-swipe-action vue-id="{{'1-'+index}}" options="{{options}}" data-event-opts="{{[['^tap',[['DeleteClick']]]]}}" onTap="__e" onVueInit="__l"><view class="ReportList"><image src="{{item.avatar}}" mode></image><view class="ReportMessage"><view class="ReportName">{{''+item.name+''}}</view><view class="ReportCon"><text>{{item.tag1}}</text><text>{{item.tag2}}</text><text>{{item.tag3}}</text></view><view class="ReportOther"><block a:for="{{item.report}}" a:for-item="con" a:for-index="i" a:key="i"><text>{{con.con}}</text></block></view></view></view></uni-swipe-action></block></view>