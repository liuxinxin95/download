<view class="content"><view class="MesssTit">学生信息</view><view data-event-opts="{{[['tap',[['changename',['$event']]]]]}}" class="Information" onTap="__e"><text>您的姓名</text><input type="text" model="{{name}}" placeholder="姓名或昵称" data-event-opts="{{[['input',[['editaddress',['$event']]]]]}}" onInput="__e"/></view><view class="Information"><text>手机号码</text><image src="../../static/arrowright.png" mode data-event-opts="{{[['tap',[['telnum',['$event']]]]]}}" onTap="__e"></image><text>{{phone}}</text></view><view class="Information"><text>选择年级</text><picker style="float:right;" value="{{grade}}" range="{{grades}}" data-event-opts="{{[['change',[['changegrade',['$event']]]]]}}" onChange="__e"><image src="../../static/arrowright.png" mode></image></picker><text>{{grade}}</text></view><view class="Information"><text>性别</text><picker style="float:right;" value="{{sex}}" range="{{sexs}}" data-event-opts="{{[['change',[['changesex',['$event']]]]]}}" onChange="__e"><image src="../../static/arrowright.png" mode></image></picker><text>{{sex}}</text></view><view class="MesssTit">学生需求</view><view class="Information"><text>老师性别</text><picker style="float:right;" value="{{sext}}" range="{{sexst}}" data-event-opts="{{[['change',[['changesext',['$event']]]]]}}" onChange="__e"><image src="../../static/arrowright.png" mode></image></picker><text>{{sext}}</text></view><view class="Information"><text>所授科目</text><picker style="float:right;" value="{{subject}}" range="{{subjects}}" data-event-opts="{{[['change',[['changesubject',['$event']]]]]}}" onChange="__e"><image src="../../static/arrowright.png" mode></image></picker><text>{{subject}}</text></view><view class="Information"><text>授课地址</text><input type="text" value="" placeholder="请输入详细地址" data-event-opts="{{[['input',[['editaddress',['$event']]]]]}}" onInput="__e"/></view><view class="Information"><text>所授时间</text><picker style="float:right;" value="{{time}}" range="{{times}}" data-event-opts="{{[['change',[['changetime',['$event']]]]]}}" onChange="__e"><image src="../../static/arrowright.png" mode></image></picker><text>{{time}}</text></view><view class="Information"><text>授课时长（小时）</text><input type="text" value="" placeholder="请输入" data-event-opts="{{[['input',[['subjecttime',['$event']]]]]}}" onInput="__e"/></view><view class="Information"><text>授课方式</text><picker style="float:right;" value="{{method}}" range="{{methods}}" data-event-opts="{{[['change',[['changemethod',['$event']]]]]}}" onChange="__e"><image src="../../static/arrowright.png" mode></image></picker><text>{{method}}</text></view><view class="Information"><text>愿支付酬金</text><input type="text" value="" placeholder="请输入" data-event-opts="{{[['input',[['money',['$event']]]]]}}" onInput="__e"/></view><view class="Information"><text>其他需求</text><input type="text" value="" placeholder="请输入" data-event-opts="{{[['input',[['other',['$event']]]]]}}" onInput="__e"/></view><view class="unloadbtn">退出登录</view></view>