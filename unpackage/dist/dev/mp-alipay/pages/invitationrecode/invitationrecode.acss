
.content{
	padding: 15px;
}
.InvitationRecodeList{
	width: 315px;
	    height: 40px;
	    -webkit-box-shadow: 2px 2px 2px #eee;
	            box-shadow: 2px 2px 2px #eee;
	    padding: 15px;
	    border-radius: 10px;
		margin-bottom: 15px;
}
.InvitationRecodeListLeft{
	width: 80%;
	    float: left;
}
.RecodeName{
	font-size: 14px;
	    color: #303331;
	    font-weight: 700;
}
.RecodeTime{
	font-size: 12px;
	    color: #9CA69F;
	    line-height: 20px;
}
.RecodeCode{
	width: 20%;
	    float: right;
	    color: #24B353;
	    font-size: 14px;
	    text-align: right;
	    display: block;
	    line-height: 40px;
}

