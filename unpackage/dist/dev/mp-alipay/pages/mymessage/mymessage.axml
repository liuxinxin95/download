<view class="content"><image class="avatarimg" src="../../static/avatar.png" mode></image><view data-event-opts="{{[['tap',[['changename',['$event']]]]]}}" class="Information" onTap="__e"><text>昵称</text><text style="float:right;color:#9DA6A0;">{{name}}</text></view><view class="Information"><text>性别</text><picker style="float:right;" value="{{sexs}}" range="{{sex}}" data-event-opts="{{[['change',[['changesex',['$event']]]]]}}" onChange="__e"><image src="../../static/arrowright.png" mode></image></picker><text>{{sexs}}</text></view><view class="Information"><text>生日</text><image src="../../static/arrowright.png" mode data-event-opts="{{[['tap',[['changebirthday',['$event']]]]]}}" onTap="__e"></image><w-picker vue-id="1" mode="date" startYear="2017" endYear="2019" defaultVal="{{[2,1,3]}}" current="{{false}}" disabledAfter="{{true}}" themeColor="#f00" ref="__r" data-ref="date" data-event-opts="{{[['^confirm',[['onConfirm']]]]}}" onConfirm="__e" onVueInit="__l"></w-picker><text style="float:right;color:rgb(157, 166, 160);">{{birthday}}</text></view><view class="Information"><text>城市</text><image src="../../static/arrowright.png" mode data-event-opts="{{[['tap',[['selectcity',['$event']]]]]}}" onTap="__e"></image><text>{{city}}</text></view><view class="Information"><text>身份</text><picker style="float:right;" value="{{identity}}" range="{{identitys}}" data-event-opts="{{[['change',[['changeidentity',['$event']]]]]}}" onChange="__e"><image src="../../static/arrowright.png" mode></image></picker><text>{{identity}}</text></view><view class="Information"><text>手机</text><image src="../../static/arrowright.png" mode data-event-opts="{{[['tap',[['telnum',['$event']]]]]}}" onTap="__e"></image><text>{{phone}}</text></view><view class="Information"><text>常住地址</text><image src="../../static/arrowright.png" mode data-event-opts="{{[['tap',[['editaddress',['$event']]]]]}}" onTap="__e"></image><text>{{address}}</text></view><view class="unloadbtn">退出登录</view></view>