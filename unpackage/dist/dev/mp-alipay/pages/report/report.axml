<view class="content"><block a:for="{{Reportlist}}" a:for-item="item" a:for-index="index" a:key="index"><view class="ReportList"><image src="{{item.avatar}}" mode></image><view class="ReportMessage"><view class="ReportName">{{''+item.name+''}}<text class="{{(item.type==1?'orangecolor':item.type==2?'redcolor':'greencolor')}}">{{item.type==1?"举报求证中":item.type==2?'举报失败':'举报成功'}}</text></view><view class="ReportCon">举报内容：<text>{{item.content}}</text></view><view class="ReportOther"><block a:for="{{item.report}}" a:for-item="con" a:for-index="i" a:key="i"><text>{{con.con}}</text></block></view></view></view></block></view>