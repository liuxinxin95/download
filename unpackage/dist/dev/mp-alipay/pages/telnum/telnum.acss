.content {
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	    -ms-flex-direction: column;
	        flex-direction: column;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	    -ms-flex-pack: center;
	        justify-content: center;
}
.logo {
	height: 290rpx;
	width: 182rpx;
	margin-top: 100rpx;
	margin-left: auto;
	margin-right: auto;
	margin-bottom: 40rpx;
}
.logintext{
	color: #303331;
	font-size: 28rpx;
	font-weight: 700;
}
.number{
	width: 570rpx;
	height: 88rpx;
	margin: 0 auto;
	background: #F6F3F3;
	border-radius: 100rpx;
	padding-left: 60rpx;
	color: #a49f9f;
	font-size: 24rpx;
	margin-top: 100rpx;
}
.code{
	height: 88rpx;
	margin-top: 30rpx;
	position: relative;
	width: 630rpx;
}
.codenum{
	width: 570rpx;
	height: 88rpx;
	margin: 0 auto;
	background: #F6F3F3;
	border-radius: 100rpx;
	padding-left: 60rpx;
	color: #a49f9f;
	font-size: 24rpx;
	position: absolute;
	top: 0;
	left: 0;
}
.code button{
	width: 180rpx;
	height: 56rpx;
	background: #24B353;
	border-radius: 100rpx;
	position: absolute;
	right: 0;
	top: 16rpx;
	color: #fff;
	font-size: 24rpx;
}
.surebtn{
	/* background: url(../../static/add.png); */
	background: #24B353;
	background-size: 100% 100%;
	width: 640rpx;
	height: 88rpx;
	margin: 0 auto;
	color: #FFFFFF;
	text-align: center;
	line-height: 88rpx;
	overflow: hidden;
	font-size: 28rpx;
	font-weight: 700;
	margin-top: 30px;
	border-radius: 50px;
}
.join{
	color: #333;
	font-size: 28rpx;
	text-align: center;
	margin-top: 40rpx;
	text-decoration: underline;
}

