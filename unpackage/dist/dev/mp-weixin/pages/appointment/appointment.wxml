<view class="content"><block wx:for="{{AppointmentListTeacher}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="{{['AppointmentListBox',identityid=='teacher'?'activeshow':'activehide']}}"><view class="AppointmentTime">{{item.Time}}</view><block wx:for="{{item.List}}" wx:for-item="list" wx:for-index="i" wx:key="i"><view class="AppointmentList"><view class="AppointmentListLeft"><text>{{list.time}}</text><text>打电话</text></view><view class="AppointmentListMid"><text class="AppointmentListName">{{list.name}}</text><view class="AppointmentListCon"><text>{{list.tag1}}</text><text>{{list.tag2}}</text><text>{{list.tag3}}</text></view></view><view data-event-opts="{{[['tap',[['AppointmentDetail',['$event']]]]]}}" class="AppointmentListRight iconfont" bindtap="__e"></view></view></block></view></block><block wx:for="{{AppointmentListStudent}}" wx:for-item="item" wx:for-index="index"><view class="{{['AppointmentListBox',identityid=='student'?'activeshow':'activehide']}}"><view class="AppointmentTime">{{item.Time}}</view><block wx:for="{{item.List}}" wx:for-item="list" wx:for-index="i" wx:key="i"><view class="AppointmentList"><view class="AppointmentListLeft"><text>{{list.time}}</text><text>打电话</text></view><view class="AppointmentListMid"><text class="AppointmentListName">{{list.name}}</text><view class="AppointmentListCons"><text>{{list.con}}</text></view></view><view data-event-opts="{{[['tap',[['AppointmentDetail',['$event']]]]]}}" class="AppointmentListRight iconfont" bindtap="__e"></view></view></block></view></block></view>