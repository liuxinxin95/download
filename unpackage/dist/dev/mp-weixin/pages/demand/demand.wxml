<view class="content"><block wx:for="{{ReportlistTearcher}}" wx:for-item="item" wx:for-index="index" wx:key="index"><uni-swipe-action class="{{[identityid=='teacher'?'activeshow':'activehide']}}" vue-id="{{'1-'+index}}" options="{{options}}" data-index="{{index}}" data-event-opts="{{[['^tap',[['DeleteClick',[index]]]]]}}" bind:tap="__e" bind:__l="__l" vue-slots="{{['default']}}"><view class="ReportList"><image src="{{item.avatar}}" mode></image><view class="ReportMessage"><view class="ReportName">{{''+item.name+''}}</view><view class="ReportCon"><text>{{item.tag1}}</text><text>{{item.tag2}}</text><text>{{item.tag3}}</text></view><view class="ReportOther"><block wx:for="{{item.report}}" wx:for-item="con" wx:for-index="i" wx:key="i"><text>{{con.con}}</text></block></view></view></view></uni-swipe-action></block><block wx:for="{{ReportlistStudent}}" wx:for-item="item" wx:for-index="index"><uni-swipe-action class="{{[identityid=='student'?'activeshow':'activehide']}}" vue-id="{{'2-'+index}}" options="{{option}}" data-index="{{index}}" data-event-opts="{{[['^tap',[['DeleteClick1',[index]]]]]}}" bind:tap="__e" bind:__l="__l" vue-slots="{{['default']}}"><view class="ReportList"><image src="{{item.avatar}}" mode></image><view class="ReportMessage"><view class="ReportName">{{''+item.name+''}}</view><view class="ReportCons"><text>{{item.content}}</text></view><view class="ReportOther"><block wx:for="{{item.report}}" wx:for-item="con" wx:for-index="i" wx:key="i"><text>{{con.con}}</text></block></view></view></view></uni-swipe-action></block><view data-event-opts="{{[['tap',[['ReleaseBtn',['$event']]]]]}}" class="release" bindtap="__e">发布需求</view></view>