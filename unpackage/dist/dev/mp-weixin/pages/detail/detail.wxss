.DetailFix{
	width: 100%;
	height: 98rpx;
	position: fixed;
	bottom: 0;
	background: #fff;
}
.ShareBtn{
	width: 15%;
	float: left;
	/* background: pink; */
	margin-left: 30rpx;
}
.ShareBtn image{
	width: 38rpx;
	height: 38rpx;
	display: block;
	margin: 0 auto;
	margin-top: 10rpx;
}
.ShareBtn text{
	color: #303331;
	font-size: 24rpx;
	text-align: center;
	display: block;
}
.ShareBtn button{
	color: #303331;
	font-size: 24rpx;
	text-align: center;
	display: block;
	border: none;
	background: #fff;
	outline: none;
	border-color: #fff;
	line-height: 40rpx;
}
.ShareBtn button::after{ border: none;}
