
.active{
	display: block;
}
.hide{
	display: none;
}
.activeshow{
	display: block;
}
.activehide{
	display: none;
}
.content{
	padding-bottom: 120rpx;
}
.DetailBack{
	width: 100%;
	height: 400rpx;
	position: relative;
}
.BackImg{
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	height: 400rpx;
	background-size: 100%;
}
.CollectionChange{
	position: absolute;
	width: 60rpx;
	height: 60rpx;
	right: 108rpx;
	top: 30rpx;
}
.CollectionChange image{
	position: absolute;
	width: 60rpx;
	height: 60rpx;
}
.ReportImg{
	position: absolute;
	width: 60rpx;
	height: 60rpx;
	right: 30rpx;
	top: 30rpx;
}
.DetailMess{
	height: 160rpx;
	    /* background: pink; */
	padding: 30rpx;
}
.DetailName{
	color: #303331;
	font-size: 28rpx;
	font-weight: 700;
	height: 30px;
	    line-height: 30px;
}
.DetailName text:nth-child(1){
	float: left;
}
.DetailName text:nth-child(2){
	float: right;
}
.DetailName text:nth-child(3){
	float: right;
	    color: #24B353;
}
.DetailCon{
	color: #9CA69F;
	font-size: 24rpx;
	height: 60rpx;
	line-height: 60rpx;
}
.DetailCons{
	color: #9CA69F;
	font-size: 24rpx;
	height: 60rpx;
	line-height: 60rpx;
}
.DetailCons text{
	overflow: hidden;
	-o-text-overflow: ellipsis;
	   text-overflow: ellipsis;
	white-space: nowrap;
	display: block;
}
.DetailCon text:nth-child(1){
	border-right: 2rpx solid #9CA69F;
	padding-right:10rpx;
}
.DetailCon text:nth-child(2){
	border-right: 2rpx solid #9CA69F;
	padding:0 10rpx;
}
.DetailCon text:nth-child(3){
	padding-left: 10rpx;
}
.DetailTag{
	font-size: 24rpx;
	color: #fff;
}
.DetailTag text:nth-child(1){
	background: #FFB726;
	padding: 6rpx 10rpx;
	border-radius: 8rpx;
	margin-right: 10rpx;
}
.DetailTag text:nth-child(2){
	background: #FF6633;
	padding: 6rpx 10rpx;
	border-radius: 8rpx;
	margin-right: 10rpx;
}
.DetailTag text:nth-child(3){
	background: #FF4D3D;
	padding: 6rpx 10rpx;
	border-radius: 8rpx;
}
.BasicMess{
	padding: 30rpx;
	-webkit-box-shadow: 4rpx 4rpx 4rpx #eee;
	        box-shadow: 4rpx 4rpx 4rpx #eee;
	width: 630rpx;
	margin: 0 auto;
}
.BasicMessList{
	height: 80rpx;
	line-height: 80rpx;
	color: #9DA6A0;
	font-size: 24rpx;
}
.BasicMessTit{
	color: #303331;
	border-left: 4rpx solid #24B353;
	font-size: 28rpx;
	padding-left: 10rpx;
	font-weight: 700;
}
.BasicMessList text{
	color: #303331;
}
