
.activeshow{
	display: block;
}
.activehide{
	display: none;
}
.changebig:hover{
	cursor: pointer;
	-webkit-transition: all 0.6s;
	-o-transition: all 0.6s;
	transition: all 0.6s;
	-webkit-transform: scale(1.1);
	    -ms-transform: scale(1.1);
	        transform: scale(1.1);
}
.content {
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	    -ms-flex-direction: column;
	        flex-direction: column;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	    -ms-flex-pack: center;
	        justify-content: center;
}
.IndexBanner{
	width: 100%;
	height: 580rpx;
	position: relative;
}
.IndexBack{
	width: 100%;
	height: 440rpx;
	position: absolute;
	top: 0;
	left: 0;
	background-size: 100%;
}
.IndexAddress{
	width: 630rpx;
	height: 310rpx;
	background: #fff;
	position: absolute;
	top: 200rpx;
	left: 30rpx;
	padding: 30rpx;
	border-radius: 20rpx;
	-webkit-box-shadow: 4rpx 4rpx 4rpx #eee;
	        box-shadow: 4rpx 4rpx 4rpx #eee;
}
.IndexCity{
	height: 110rpx;
	line-height: 110rpx;
	font-size: 28rpx;
	color: #303331;
	border-bottom: 2rpx solid #F0F5F2;
	padding: 0 30rpx;
}
.IndexCity text:nth-child(2){
	float: right;
}
.IndexCityImg{
	float: right;
	position: relative;
	width: 34rpx;
	height: 34rpx;
}
.IndexCityImg image{
	width: 34rpx;
	height: 34rpx;
	top: 36rpx;
	right: 16rpx;
	position: absolute;
}
.IndexAddr{
	height: 120rpx;
	font-size: 28rpx;
	line-height: 120rpx;
	color: #303331;
}
.IndexAddr input{
	height: 120rpx;
	line-height: 120rpx;
	width: 80%;
	float: left;
	padding: 0 30rpx;
}
.IndexAddr image{
	width: 34rpx;
	height: 34rpx;
	background-size: 100%;
	margin-top: 46rpx;
}
.IndexSearch{
	width: 630rpx;
	height: 88rpx;
	background: #24B353;
	color: #fff;
	font-size: 28rpx;
	text-align: center;
	line-height: 88rpx;
	border-radius: 100rpx;
}
/* 分类 */
.IndexSort{
	height: 130rpx;
	width: 100%;
	margin-top: 80rpx;
}
.IndexSortList{
	width: 20%;
	float: left;
}
.IndexSortList image{
	width: 82rpx;
	height: 82rpx;
	margin: 0 auto;
	display: block;
}
.IndexSortList text{
	color: #303331;
	font-size: 24rpx;
	text-align: center;
	display: block;
	margin-top: 18rpx;
}
/* 数据成就 */
.IndexData{
	margin-top: 60rpx;
	width: 100%;
}
.IndexDataTit{
	height: 100rpx;
	line-height: 100rpx;
	padding: 0 30rpx;
	font-weight: 700;
	font-size: 38rpx;
}
.IndexDataCon{
	-webkit-box-shadow: 4rpx 4rpx 4rpx #eee;
	        box-shadow: 4rpx 4rpx 4rpx #eee;
	width: 690rpx;
	height: 150rpx;
	margin: 0 auto;
}
.IndexDataList{
	width: 33.33%;
	float: left;
}
.IndexDataList text:nth-child(1){
	color: #303331;
	font-size: 38rpx;
	font-weight: 700;
	text-align: center;
	display: block;
	margin-top: 40rpx;
}
.IndexDataList text:nth-child(2){
	color: #9DA6A0;
	font-size: 24rpx;
	text-align: center;
	display: block;
}
/* 精选名师 */
.IndexGood{
	margin-top: 60rpx;
}
.IndexGoodTit{
	height: 100rpx;
	line-height: 100rpx;
}
.IndexGoodTit text:nth-child(1){
	color: #303331;
	font-size: 38rpx;
	font-weight: 700;
	float: left;
}
.IndexGoodTit text:nth-child(2){
	float: right;
	color: #303331;
	font-size: 24rpx;
	width: 80rpx;
	height: 40rpx;
	border: 2rpx solid #eee;
	border-radius: 100rpx;
	line-height: 40rpx;
	text-align: center;
	margin-top: 26rpx;
}
.ReportList{
	width: 630rpx;
	height: 160rpx;
	padding: 30rpx;
	-webkit-box-shadow: 4rpx 4rpx 4rpx #eee;
	        box-shadow: 4rpx 4rpx 4rpx #eee;
	background: #FFFFFF;
	border-radius: 16rpx;
	margin-bottom: 30rpx;
}
.ReportList image{
	width: 160rpx;
	height: 160rpx;
	border-radius: 20rpx;
	margin-right: 32rpx;
	float: left;
}
.ReportMessage{
	width: 60%;
	float: left;
}
.ReportName{
	color: #303331;
	font-size: 28rpx;
	font-weight: 700;
	height: 50rpx;
	line-height: 50rpx;
}
.ReportName text:nth-child(1){
	font-size: 28rpx;
	float: left;
}
.ReportName text:nth-child(2){
	float: right;
	font-size: 24rpx;
	color: #24B353;
}
.ReportName text:nth-child(3){
	float: right;
	font-size: 24rpx;
	color: #24B353;
}
.ReportCon{
	color: #9CA69F;
	font-size: 28rpx;
	line-height: 60rpx;
}
.ReportCons{
	color: #9CA69F;
	font-size: 28rpx;
	line-height: 60rpx;
}
.ReportCon text:nth-child(1){
	border-right: 2rpx solid #9CA69F;
	padding-right: 10rpx;
}
.ReportCon text:nth-child(2){
	padding: 0 10rpx;
	border-right: 2rpx solid #9CA69F;
}
.ReportCon text:nth-child(3){
	padding-left: 10rpx;
}
.ReportOther{
	margin-top: 20rpx;
}
.ReportOther text{
	font-size: 24rpx;
	color: #fff;
	/* background: red; */
	padding: 4rpx 10rpx;
	border-radius: 10rpx;
	margin-right: 10rpx;
	float: left;
}
.ReportOther text:nth-child(1){
	background: #FFB726;
}
.ReportOther text:nth-child(2){
	background: #FF6633;
}
.ReportOther text:nth-child(3){
	background: #FF4D3D;
}
.ReportName .orangecolor{
	color: #FFB726;
	border-color: #FFB726;
}
.ReportName .redcolor{
	color: #FF4D3D;
	border-color: #FF4D3D;
}
.ReportName .greencolor{
	color: #24B353;
	border-color: #24B353;
}
