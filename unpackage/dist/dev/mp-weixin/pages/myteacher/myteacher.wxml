<view class="content"><block wx:for="{{ReportlistTearcher}}" wx:for-item="item" wx:for-index="index" wx:key="index"><uni-swipe-action vue-id="{{'1-'+index}}" options="{{options}}" data-index="{{index}}" data-event-opts="{{[['^tap',[['DeleteClick',[index]]]]]}}" bind:tap="__e" bind:__l="__l" vue-slots="{{['default']}}"><view class="ReportList"><image src="{{item.avatar}}" mode></image><view class="ReportMessage"><view class="ReportName">{{''+item.name+''}}<text data-event-opts="{{[['tap',[['reportclick',['$0'],[[['ReportlistTearcher','',index,'states']]]]]]]}}" class="greencolor" bindtap="__e">{{item.states==0?'缴费':'提醒'}}</text></view><view class="ReportCon"><text>{{item.tag1}}</text><text>{{item.tag2}}</text><text>{{item.tag3}}</text></view><view class="ReportOther"><block wx:for="{{item.report}}" wx:for-item="con" wx:for-index="i" wx:key="i"><text>{{con.con}}</text></block></view></view></view></uni-swipe-action></block><view data-event-opts="{{[['tap',[['AddTeacher',['$event']]]]]}}" class="AddTeacher" bindtap="__e">添加老师</view><uni-popup class="vue-ref" vue-id="2" type="center" data-ref="popup" bind:__l="__l" vue-slots="{{['default']}}"><view class="aa"><view class="addpeople">添加老师</view><input class="inputcode" type="text" placeholder="请输入老师的邀请码" model="{{code}}" data-event-opts="{{[['input',[['inputcode',['$event']]]]]}}" bindinput="__e"/><view class="twobtn"><view data-event-opts="{{[['tap',[['closePopup',['$event']]]]]}}" class="cancel" bindtap="__e">取消</view><view data-event-opts="{{[['tap',[['surePopup',['$event']]]]]}}" class="sure" bindtap="__e">确定</view></view></view></uni-popup><uni-popup class="vue-ref" vue-id="3" type="center" data-ref="popups" bind:__l="__l" vue-slots="{{['default']}}"><view class="bb"><view class="surepaytit">需支付老师认证费用，确保信息正式可靠。</view><view class="twobtn"><view data-event-opts="{{[['tap',[['closePopups',['$event']]]]]}}" class="cancel" bindtap="__e">取消</view><view data-event-opts="{{[['tap',[['surePopups',['$event']]]]]}}" class="sure" bindtap="__e">去支付</view></view></view></uni-popup></view>