<view class="contents"><view class="contenttop"><view data-event-opts="{{[['tap',[['shownear',['$event']]]]]}}" class="contenttoplist nearby" bindtap="__e"><text>附近</text><text class="iconfont">{{aa==true?'':''}}</text></view><view data-event-opts="{{[['tap',[['showgrade',['$event']]]]]}}" class="contenttoplist grade" bindtap="__e"><text>年级</text><text class="iconfont">{{bb==true?'':''}}</text></view><view data-event-opts="{{[['tap',[['showsubject',['$event']]]]]}}" class="contenttoplist subject" bindtap="__e"><text>科目</text><text class="iconfont">{{cc==true?'':''}}</text></view><view data-event-opts="{{[['tap',[['showfilter',['$event']]]]]}}" class="contenttoplist filter" bindtap="__e"><text>筛选</text><text class="iconfont">{{dd==true?'':''}}</text></view></view><uni-popup class="vue-ref" vue-id="1" type="top" data-ref="popupnearby" bind:__l="__l" vue-slots="{{['default']}}"><view class="nearbycon"><view class="nearbylist"><block wx:for="{{nearbys}}" wx:for-item="item" wx:for-index="index" wx:key="index"><text class="{{[current==index?'check':'']}}" data-index="{{index}}" data-event-opts="{{[['tap',[['nearby',['$0',index],[[['nearbys','',index]]]]]]]}}" bindtap="__e">{{item.name}}</text></block></view></view></uni-popup><uni-popup class="vue-ref" vue-id="2" type="top" data-ref="popupgrade" bind:__l="__l" vue-slots="{{['default']}}"><view class="gradecon"><block wx:for="{{grades}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="gradelist"><view class="gradelisttit">{{''+item.name+''}}</view><view class="gradelistcon"><block wx:for="{{item.gradeslist}}" wx:for-item="items" wx:for-index="index1" wx:key="index1"><text class="{{[current1=='a'+index+'b'+index1?'check':'']}}" data-index="{{index1}}" data-event-opts="{{[['tap',[['grade',['$0',index1,index],[[['grades','',index],['gradeslist','',index1]]]]]]]}}" bindtap="__e">{{items.con}}</text></block></view></view></block></view></uni-popup><uni-popup class="vue-ref" vue-id="3" type="top" data-ref="popupsubject" bind:__l="__l" vue-slots="{{['default']}}"><view class="nearbycon"><view class="nearbylist"><block wx:for="{{subjects}}" wx:for-item="item" wx:for-index="index" wx:key="index"><text class="{{[current2==index?'check':'']}}" data-index="{{index}}" data-event-opts="{{[['tap',[['subject',['$0',index],[[['subjects','',index]]]]]]]}}" bindtap="__e">{{item.name}}</text></block></view></view></uni-popup><uni-popup class="vue-ref" vue-id="4" type="top" data-ref="popupfilter" bind:__l="__l" vue-slots="{{['default']}}"><view class="gradecon"><block wx:for="{{filters}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="gradelist"><view class="gradelisttit">{{''+item.name+''}}</view><view class="gradelistcon"><block wx:for="{{item.filterlist}}" wx:for-item="items" wx:for-index="indexa" wx:key="indexa"><text data-index="{{indexa}}" data-event-opts="{{[['tap',[['filter',['$0',indexa,index],[[['filters','',index],['filterlist','',indexa]]]]]]]}}" bindtap="__e">{{items.con}}</text></block></view></view></block><view class="selectbtn"><view data-event-opts="{{[['tap',[['reset',['$event']]]]]}}" class="reset" bindtap="__e">重置</view><view data-event-opts="{{[['tap',[['surebtn',['$event']]]]]}}" class="sure" bindtap="__e">确定</view></view></view></uni-popup><block wx:for="{{ReportlistTeacher}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="{{['ReportList',identityid=='student'?'activeshow':'activehide']}}"><image src="{{item.avatar}}" mode data-event-opts="{{[['tap',[['MapGo',['$0'],[[['ReportlistTeacher','',index]]]]]]]}}" bindtap="__e"></image><view data-event-opts="{{[['tap',[['Detail',['$event']]]]]}}" class="ReportMessage" bindtap="__e"><view class="ReportName"><text>{{item.name}}</text><text>{{item.distance}}</text></view><view class="ReportCon"><text>{{item.tag1}}</text><text>{{item.tag2}}</text><text>{{item.tag3}}</text></view><view class="ReportOther"><block wx:for="{{item.report}}" wx:for-item="con" wx:for-index="i" wx:key="i"><text>{{con.con}}</text></block></view></view></view></block></view>